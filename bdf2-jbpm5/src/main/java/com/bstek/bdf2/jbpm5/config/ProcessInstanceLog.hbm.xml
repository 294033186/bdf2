<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jbpm.process.audit">

	<class name="ProcessInstanceLog" table="JBPM5_PROCESS_INSTANCE_LOG">
        <id name="id" type="long" column="ID_">
            <generator class="native"/>
        </id>
        <property name="processInstanceId" column="PROCESS_INSTANCE_ID_"/>
        <property name="processId" column="PROCESS_ID_"/>
        <property name="start" type="timestamp" column="START_DATE"/>
        <property name="end" type="timestamp" column="END_DATE_"/>
    </class>

</hibernate-mapping>
